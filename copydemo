# .bashrc

# User specific aliases and functions

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

alias dockill='docker kill $(docker ps -qa);docker rm $(docker ps -qa)'
alias python=python2.7
# first go to cd /root/Downloads/pycharm-community-2018.1.3/bin
alias pycharm=./pycharm.sh

alias c=clear


JAVA_HOME=/usr/java/jdk1.8.0_121
PATH=$JAVA_HOME/bin:$PATH
export PATH

